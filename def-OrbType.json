    "AsteroidFieldOrbTypeId": {
      "$ref": "#/$defs/IntegerExpression"
    },
    "StarProbability": {
      "$ref": "#/$defs/OrbTypeFactor"
    },
    "ChildTypes": {
      "oneOf": [
        {
          "type": "array",
          "items": {
            "oneOf": [
              {
                "$ref": "#/$defs/OrbTypeFactor"
              },
              {
                "type": "string",
                "pattern": "^\\(delete\\)$"
              }
            ]
          }
        },
        {
          "type": "object",
          "patternProperties": {
            "^0|[1-9][0-9]*|\\([^\\)]\u002B\\)$": {
              "oneOf": [
                {
                  "$ref": "#/$defs/OrbTypeFactor"
                },
                {
                  "type": "string",
                  "pattern": "^\\(delete\\)$"
                }
              ]
            }
          },
          "$ref": "./expression-language.json#/$defs/list-selection"
        },
        {
          "type": "object",
          "properties": {
            "$add": {
              "type": "array",
              "items": {
                "$ref": "#/$defs/OrbTypeFactor"
              }
            }
          }
        }
      ]
