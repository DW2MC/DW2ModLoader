    "EnergyOutputMinimum": {
      "$ref": "#/$defs/NumberExpression"
    },
    "EnergyOutputMaximum": {
      "$ref": "#/$defs/NumberExpression"
    },
    "LocationEffectId": {
      "$ref": "#/$defs/IntegerExpression"
    },
    "CommonBonuses": {
      "oneOf": [
        {
          "type": "array",
          "items": {
            "oneOf": [
              {
                "$ref": "#/$defs/BonusRange"
              },
              {
                "type": "string",
                "pattern": "^\\(delete\\)$"
              }
            ]
          }
        },
        {
          "type": "object",
          "patternProperties": {
            "^0|[1-9][0-9]*|\\([^\\)]\u002B\\)$": {
              "oneOf": [
                {
                  "$ref": "#/$defs/BonusRange"
                },
                {
                  "type": "string",
                  "pattern": "^\\(delete\\)$"
                }
              ]
            }
          },
          "$ref": "./expression-language.json#/$defs/list-selection"
        },
        {
          "type": "object",
          "properties": {
            "$add": {
              "type": "array",
              "items": {
                "$ref": "#/$defs/BonusRange"
              }
            }
          }
        }
      ]
    },
    "RingsProbability": {
      "$ref": "#/$defs/NumberExpression"
    },
    "RingsPrimaryColor": {
      "$ref": "#/$defs/Color"
    },
    "RingsColorVariationFactor": {
      "$ref": "#/$defs/NumberExpression"
    },
    "AsteroidFieldProbability": {
      "$ref": "#/$defs/NumberExpression"
